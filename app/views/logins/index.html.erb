
<h1>Log in</h1>

<%= flash[:notice] %>

<%= form_for :user, @user, :url => { :action=>"create", :controller=>"logins"} do |f|%>
  <p>
	<%= f.label :email_id %><br/>
	<%= f.text_field :email_id %>
  </p>
  <p>
	<%= f.label :password %><br/>
	<%= f.password_field :password%>
  </p>
  <p><%= f.submit  "Login"%></p>
<% end %> 

<%= link_to "Back", %>
 